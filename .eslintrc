{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "ignorePatterns": ["build/**/*", "jest.config.js"],
  "rules": {
    "no-console": "error",
    "max-len": ["error", { "code": 120 }]
  }
}
